{
    "version":2,
    "builds":[
         {
             "src":"frontend/package.json",
             "use":"@vercel/static-build",
             "config": { "distDir": "dist" }
         }
    ],
    "rewrites":[
        { "source":"/api/(.*)", "destination":"https://your-backend-url.com/api/$1" },
        { "source":"/(.*)", "destination":"/$1" }
    ]
}